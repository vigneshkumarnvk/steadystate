    <lightning:card title="{!v.title}">
        <aura:if isTrue="{!v.salesOrder.Document_Type__c == 'Sales Quote'}">
            <!-- Standard Assumptions & Contingencies -->
            <lightning:layoutItem size="12" padding="horizontal-small">
                <lightning:card title="Standard Assumptions &amp; Contingencies">
                    <p class="slds-p-horizontal_small">
                        <lightning:input type="checkbox" checked="{!v.salesOrder.Free_Access__c}"
                                         label="{!v.salesOrder.Service_Center__r.Standard_AC_Opt1__c == null ? 'Republic Services will have free and easy access to the work area.' : v.salesOrder.Service_Center__r.Standard_AC_Opt1__c}"
                                         disabled="{!v.salesOrder.Document_Status__c == 'Closed' || v.salesOrder.Approval_Status__c == 'Pending_Approval' || v.salesOrder.Expired__c == true || v.salesOrder.Blocked__c == true || (v.salesOrder.Allow_Edit__c != true &amp;&amp; v.salesOrder.Id != null)}"/>

                        <lightning:input type="checkbox" checked="{!v.salesOrder.Pricing__c}"
                                         label="{!v.salesOrder.Service_Center__r.Standard_AC_Opt2__c == null ? 'Pricing is based on work being completed during normal business hours Monday - Friday.' : v.salesOrder.Service_Center__r.Standard_AC_Opt2__c}"
