                                         label="{!v.salesOrder.Service_Center__r.Standard_AC_Opt3__c == null ? 'Any expendables used are charged in accordance with Republic Services\' current expendable price list.' : v.salesOrder.Service_Center__r.Standard_AC_Opt3__c}"
                                         disabled="{!v.salesOrder.Document_Status__c == 'Closed' || v.salesOrder.Approval_Status__c == 'Pending_Approval' || v.salesOrder.Expired__c == true || v.salesOrder.Blocked__c == true || (v.salesOrder.Allow_Edit__c != true &amp;&amp; v.salesOrder.Id != null)}"/>

                        <lightning:input type="checkbox" checked="{!v.salesOrder.Applicable_Taxes__c}"
                                         label="{!v.salesOrder.Service_Center__r.Standard_AC_Opt4__c == null ? 'Applicable local, state, federal, transporter and/or other applicable taxes or fees will be added to the invoice.' : v.salesOrder.Service_Center__r.Standard_AC_Opt4__c}"
                                         disabled="{!v.salesOrder.Document_Status__c == 'Closed' || v.salesOrder.Approval_Status__c == 'Pending_Approval' || v.salesOrder.Expired__c == true || v.salesOrder.Blocked__c == true || (v.salesOrder.Allow_Edit__c != true &amp;&amp; v.salesOrder.Id != null)}"/>
