<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <submitter>Approvers</submitter>
        <type>group</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>hramos@acvenviro.com</submitter>
        <type>user</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>tperez@acvenviro.com</submitter>
        <type>user</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>Order_Cost__c</field>
        <field>Delivery_Contact_Name__c</field>
        <field>Expected_Receipt_Date__c</field>
        <field>Order_Date__c</field>
        <field>Buy_from_Vendor__c</field>
        <field>Note__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>adhoc</type>
            </approver>
        </assignedApprover>
        <label>Approver</label>
        <name>Approver_1</name>
    </approvalStep>
    <emailTemplate>unfiled$public/PO_Approval_Notification_Email_VF</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <finalApprovalActions>
        <action>
            <name>MarkStepCompleted</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Rejected_PO_Notification</name>
            <type>Alert</type>
        </action>
        <action>
            <name>SetApprovalStatusToZero</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>SetPendingApprovalStatustoNull</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>SetStatusToOpen</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <label>SC15_REG</label>
    <processOrder>1</processOrder>
    <recallActions>
        <action>
            <name>UpdateRecallStatus</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
