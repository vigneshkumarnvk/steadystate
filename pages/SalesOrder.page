                <apex:outputField value="{!salesOrder.Quote_Date__c}"  rendered="{!!open}"/>

                                <apex:pageBlockSectionItem >
                    <apex:outputLabel >{!$ObjectType.Sales_Order__c.Fields.Surcharge_Type__c.Label}</apex:outputLabel>
                    <apex:outputpanel >
