                <apex:outputField value="{!salesOrder.CMR_Description__c}" rendered="{!NOT(open) && salesOrder.Document_Type__c=='Sales Order'}"/>
                <apex:inputField id="payRule" value="{!salesOrder.Pay_Rule__c}"
